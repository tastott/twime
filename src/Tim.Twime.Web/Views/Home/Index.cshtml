@{
    ViewBag.Title = "twime";
}



<!-- Main jumbotron for a primary marketing message or call to action -->
<div class="jumbotron">
    <h1>"The Wind is my <strong>Enemy.</strong>"</h1>
    <p>You would have gone <i>so</i> much faster without that stupid wind.</p>
    <p>Upload a GPX of your blustery ride to salve your damaged ego.</p>
</div>
<div id="upload">
    <form action="@Url.Action("UploadAndAnalyse")" method="post" enctype="multipart/form-data">
        @Html.Partial("_UploadInputs")
        <button type="submit" class="btn btn-primary btn-lg" role="button">Upload</button>
    </form>
</div>
